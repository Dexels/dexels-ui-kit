{
    "name": "dexels-ui-kit",
    "version": "2.3.5",
    "author": {
        "name": "Dexels B.V.",
        "email": "info@dexels.com"
    },
    "description": "An UI kit which can be used in Dexels applications",
    "main": "index.js",
    "module": "index.esm.js",
    "types": "index.d.ts",
    "files": [
        "index.d.ts",
        "index.js",
        "index.esm.js",
        "fonts",
        "main.css"
    ],
    "scripts": {
        "start": "npm run storybook",
        "build:prod": "npm run clean && cross-env BABEL_ENV=prod rollup --config config/rollup.config.js && npm run generate:types",
        "check-types": "tsc",
        "dist": "npm run build:prod",
        "dist:analyze": "npm run build:prod -- --environment analyze",
        "generate:types": "./node_modules/.bin/dts-bundle-generator --config=dtsconfig.json",
        "generate:iconTypes": "node ./scripts/generateIconTypes.js",
        "lint:css": "stylelint \"src/**/*.sc.ts\"",
        "lint:js": "eslint \"src\" --ext \".js,.ts,.tsx\"",
        "storybook": "start-storybook -c ./src/.storybook -s ./public --port 3434 --quiet",
        "storybook:build": "build-storybook -c ./src/.storybook -s ./public -o .out",
        "prepublishOnly": "npx rimraf node_modules && npm i && npm run dist",
        "postinstall": "npx rimraf node_modules/@types/react-native",
        "postpublish": "npm run clean",
        "clean": "npx rimraf index.d.ts index.js index.esm.js fonts main.css stats.html"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Dexels/dexels-ui-kit.git"
    },
    "keywords": [
        "UI kit",
        "React",
        "Styled Components"
    ],
    "browserslist": [
        "Last 1 Chrome versions",
        "Last 1 Edge versions",
        "Last 1 Firefox versions"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Dexels/dexels-ui-kit/issues"
    },
    "homepage": "https://github.com/Dexels/dexels-ui-kit#readme",
    "peerDependencies": {
        "@babel/runtime": "^7.8.4",
        "@types/react": "^16.9.19",
        "@types/react-dates": "^17.1.9",
        "@types/react-table": "^7.0.4",
        "@types/styled-components": "^4.4.2",
        "moment": "^2.24.0",
        "react": "^16.12.0",
        "react-dates": "^21.8.0",
        "react-dom": "^16.12.0",
        "react-loading-skeleton": "^2.0.1",
        "react-router-dom": "^5.1.2",
        "react-table": "^7.0.0-rc.15",
        "react-with-direction": "^1.3.1",
        "styled-components": "^5.0.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.8.4",
        "@babel/core": "^7.8.4",
        "@babel/preset-env": "^7.8.4",
        "@babel/preset-react": "^7.8.3",
        "@babel/runtime": "^7.8.4",
        "@rollup/plugin-commonjs": "^11.0.2",
        "@rollup/plugin-node-resolve": "^7.1.1",
        "@storybook/addon-actions": "^5.3.12",
        "@storybook/addon-backgrounds": "^5.3.12",
        "@storybook/addon-info": "^5.3.12",
        "@storybook/addon-knobs": "^5.3.12",
        "@storybook/addon-notes": "^5.3.12",
        "@storybook/react": "^5.3.12",
        "@types/react": "^16.9.19",
        "@types/react-dates": "^17.1.9",
        "@types/react-dom": "^16.9.5",
        "@types/react-router-dom": "^5.1.3",
        "@types/react-table": "^7.0.4",
        "@types/styled-components": "^4.4.2",
        "@typescript-eslint/eslint-plugin": "^2.19.0",
        "@typescript-eslint/parser": "^2.19.0",
        "awesome-typescript-loader": "^5.2.1",
        "babel-eslint": "^10.0.3",
        "babel-loader": "^8.0.6",
        "babel-plugin-styled-components": "^1.10.7",
        "cross-env": "^7.0.0",
        "dts-bundle-generator": "^3.3.1",
        "eslint": "^6.8.0",
        "eslint-config-airbnb": "^18.0.1",
        "eslint-config-airbnb-typescript": "^6.3.2",
        "eslint-plugin-babel": "^5.3.0",
        "eslint-plugin-import": "^2.20.1",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-react": "^7.18.3",
        "eslint-plugin-react-hooks": "^2.3.0",
        "eslint-plugin-typescript-sort-keys": "^0.5.0",
        "moment": "^2.24.0",
        "postcss-url": "^8.0.0",
        "react": "^16.12.0",
        "react-dates": "^21.8.0",
        "react-dom": "^16.12.0",
        "react-loading-skeleton": "^2.0.1",
        "react-router-dom": "^5.1.2",
        "react-table": "^7.0.0-rc.15",
        "react-with-direction": "^1.3.1",
        "rollup": "^1.31.0",
        "rollup-plugin-babel": "^4.3.3",
        "rollup-plugin-copy": "^3.3.0",
        "rollup-plugin-postcss": "^2.0.6",
        "rollup-plugin-terser": "^5.2.0",
        "rollup-plugin-typescript2": "^0.25.3",
        "rollup-plugin-visualizer": "^3.3.1",
        "storybook-addon-styled-component-theme": "^1.3.0",
        "styled-components": "^5.0.1",
        "stylelint": "^13.1.0",
        "stylelint-config-concentric-order": "^4.0.0",
        "stylelint-config-recommended": "^3.0.0",
        "stylelint-config-styled-components": "^0.1.1",
        "stylelint-junit-formatter": "^0.2.2",
        "typescript": "^3.7.5"
    }
}
