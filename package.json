{
  "name": "dexels-ui-kit",
  "version": "1.0.0",
  "description": "An UI kit which can be used in Dexels applications",
  "main": "src/index.js",
  "scripts": {
    "start": "npm run build:dev",
    "build:dev": "cross-env BABEL_ENV=dev webpack-dev-server --hot --config config/webpack.dev.js --mode development",
    "build:prod": "cross-env BABEL_ENV=prod webpack --config config/webpack.prod.js --mode production",
    "dist": "npm run build:prod",
    "lint:css": "stylelint './src/**/*.js' --config config/.stylelintrc.js",
    "lint:js": "eslint './src/**/*.js' --config config/.eslintrc.js",
    "storybook": "start-storybook --config-dir src/.storybook --quiet"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dexels/dexels-ui-kit.git"
  },
  "keywords": [
    "UI kit",
    "React",
    "Styled Components"
  ],
  "author": "Dexels BV",
  "browserslist": [
    "Last 1 Chrome versions",
    "Last 1 Edge versions",
    "Last 1 Firefox versions"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Dexels/dexels-ui-kit/issues"
  },
  "homepage": "https://github.com/Dexels/dexels-ui-kit#readme",
  "dependencies": {
    "@hot-loader/react-dom": "^16.9.0",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-hot-loader": "^4.12.13",
    "styled-components": "^4.3.2",
    "styled-reset": "^4.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^5.2.1",
    "@storybook/addon-knobs": "^5.2.1",
    "@storybook/react": "^5.2.1",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^6.0.0",
    "css-loader": "^3.2.0",
    "eslint": "^6.4.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "style-loader": "^1.0.0",
    "stylelint": "^11.0.0",
    "stylelint-config-concentric-order": "^3.4.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.8.0",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1",
    "webpack-merge": "^4.2.2"
  }
}
