{
    "name": "dexels-ui-kit",
    "version": "2.1.6",
    "author": {
        "name": "Dexels B.V.",
        "email": "info@dexels.com"
    },
    "description": "An UI kit which can be used in Dexels applications",
    "main": "index.js",
    "module": "index.esm.js",
    "types": "index.d.ts",
    "files": [
        "index.d.ts",
        "index.js",
        "index.esm.js",
        "fonts",
        "main.css"
    ],
    "scripts": {
        "start": "npm run storybook",
        "build:prod": "npm run clean && cross-env BABEL_ENV=prod rollup --config config/rollup.config.js",
        "dist": "npm run build:prod",
        "dist:analyze": "npm run build:prod -- --environment analyze",
        "lint:css": "stylelint \"src/**/*.js\"",
        "lint:js": "eslint \"src/**/*.js\" --config config/.eslintrc.js",
        "storybook": "start-storybook -c ./src/.storybook -s ./public --port 3434 --quiet",
        "storybook:build": "build-storybook -c ./src/.storybook -s ./public -o .out",
        "prepublishOnly": "npx rimraf node_modules && npm i && npm run dist",
        "postpublish": "npm run clean",
        "check-types": "tsc",
        "clean": "npx rimraf index.d.ts index.js index.esm.js fonts main.css stats.html"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Dexels/dexels-ui-kit.git"
    },
    "keywords": [
        "UI kit",
        "React",
        "Styled Components"
    ],
    "browserslist": [
        "Last 1 Chrome versions",
        "Last 1 Edge versions",
        "Last 1 Firefox versions"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Dexels/dexels-ui-kit/issues"
    },
    "homepage": "https://github.com/Dexels/dexels-ui-kit#readme",
    "peerDependencies": {
        "@babel/runtime": "^7.8.3",
        "@types/react": "^16.9.17",
        "@types/react-dates": "^17.1.7",
        "moment": "^2.24.0",
        "prop-types": "^15.7.2",
        "react": "^16.12.0",
        "react-dates": "^21.6.0",
        "react-dom": "^16.12.0",
        "react-router-dom": "^5.1.2",
        "react-table": "^7.0.0-rc.15",
        "react-with-direction": "^1.3.1",
        "styled-components": "^5.0.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.8.3",
        "@babel/core": "^7.8.3",
        "@babel/preset-env": "^7.8.3",
        "@babel/preset-react": "^7.8.3",
        "@babel/runtime": "^7.8.3",
        "@rollup/plugin-commonjs": "^11.0.1",
        "@rollup/plugin-node-resolve": "^7.0.0",
        "@storybook/addon-actions": "^5.3.7",
        "@storybook/addon-backgrounds": "^5.3.7",
        "@storybook/addon-info": "^5.3.7",
        "@storybook/addon-knobs": "^5.3.7",
        "@storybook/addon-notes": "^5.3.7",
        "@storybook/react": "^5.3.7",
        "@types/react": "^16.9.17",
        "@types/react-dates": "^17.1.7",
        "babel-eslint": "^10.0.3",
        "babel-loader": "^8.0.6",
        "babel-plugin-styled-components": "^1.10.6",
        "cross-env": "^6.0.3",
        "eslint": "^6.8.0",
        "eslint-config-airbnb": "^18.0.1",
        "eslint-plugin-babel": "^5.3.0",
        "eslint-plugin-import": "^2.20.0",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-react": "^7.18.0",
        "eslint-plugin-react-hooks": "^2.3.0",
        "moment": "^2.24.0",
        "postcss-url": "^8.0.0",
        "prop-types": "^15.7.2",
        "react": "^16.12.0",
        "react-dates": "^21.7.0",
        "react-dom": "^16.12.0",
        "react-moment-proptypes": "^1.7.0",
        "react-router-dom": "^5.1.2",
        "react-table": "^7.0.0-rc.15",
        "react-with-direction": "^1.3.1",
        "rollup": "^1.29.1",
        "rollup-plugin-babel": "^4.3.3",
        "rollup-plugin-copy": "^3.2.1",
        "rollup-plugin-postcss": "^2.0.3",
        "rollup-plugin-terser": "^5.2.0",
        "rollup-plugin-visualizer": "^3.3.1",
        "storybook-addon-styled-component-theme": "^1.3.0",
        "styled-components": "^5.0.0",
        "stylelint": "^13.0.0",
        "stylelint-config-concentric-order": "^3.5.0",
        "stylelint-config-recommended": "^3.0.0",
        "stylelint-config-styled-components": "^0.1.1",
        "stylelint-junit-formatter": "^0.2.2",
        "typescript": "^3.7.5"
    }
}
