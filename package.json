{
    "name": "dexels-ui-kit",
    "version": "1.0.24",
    "author": {
        "name": "Dexels B.V.",
        "email": "info@dexels.com"
    },
    "description": "An UI kit which can be used in Dexels applications",
    "main": "dist/index.js",
    "files": [
        "dist"
    ],
    "scripts": {
        "start": "npm run storybook",
        "build:prod": "cross-env BABEL_ENV=prod webpack --config config/webpack.config.js",
        "dist": "npm run build:prod",
        "dist:analyze": "npm run build:prod -- --env.analyze=true",
        "lint:css": "stylelint \"src/**/*.js\" --config config/.stylelintrc.js",
        "lint:js": "eslint \"src/**/*.js\" --config config/.eslintrc.js",
        "storybook": "start-storybook -c ./src/.storybook -s ./public --port 3434 --quiet",
        "storybook:build": "build-storybook -c ./src/.storybook -s ./public -o .out",
        "prepublishOnly": "rm -rf node_modules && npm i && npm run dist"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Dexels/dexels-ui-kit.git"
    },
    "keywords": [
        "UI kit",
        "React",
        "Styled Components"
    ],
    "browserslist": [
        "Last 1 Chrome versions",
        "Last 1 Edge versions",
        "Last 1 Firefox versions"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Dexels/dexels-ui-kit/issues"
    },
    "homepage": "https://github.com/Dexels/dexels-ui-kit#readme",
    "peerDependencies": {
        "moment": "^2.24.0",
        "prop-types": "^15.7.2",
        "react": "^16.10.2",
        "react-dates": "^21.2.1",
        "react-dom": "^16.10.2",
        "styled-components": "^4.4.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.7.0",
        "@babel/core": "^7.7.2",
        "@babel/preset-env": "^7.7.1",
        "@babel/preset-react": "^7.7.0",
        "@babel/runtime": "^7.7.2",
        "@storybook/addon-actions": "^5.2.5",
        "@storybook/addon-backgrounds": "^5.2.5",
        "@storybook/addon-info": "^5.2.5",
        "@storybook/addon-knobs": "^5.2.5",
        "@storybook/react": "^5.2.5",
        "babel-eslint": "^10.0.3",
        "babel-loader": "^8.0.6",
        "babel-plugin-styled-components": "^1.10.6",
        "clean-webpack-plugin": "^3.0.0",
        "cross-env": "^6.0.3",
        "css-loader": "^3.2.0",
        "eslint": "^6.6.0",
        "eslint-config-airbnb": "^18.0.1",
        "eslint-plugin-babel": "^5.3.0",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-react": "^7.16.0",
        "eslint-plugin-react-hooks": "^2.2.0",
        "file-loader": "^4.2.0",
        "mini-css-extract-plugin": "^0.8.0",
        "moment": "^2.24.0",
        "moment-locales-webpack-plugin": "^1.1.0",
        "optimize-css-assets-webpack-plugin": "^5.0.3",
        "prop-types": "^15.7.2",
        "react": "^16.11.0",
        "react-dates": "^21.3.2",
        "react-dom": "^16.11.0",
        "react-moment-proptypes": "^1.7.0",
        "react-with-direction": "^1.3.1",
        "storybook-addon-styled-component-theme": "^1.2.5",
        "styled-components": "^4.4.1",
        "stylelint": "^11.1.1",
        "stylelint-config-concentric-order": "^3.5.0",
        "stylelint-config-recommended": "^3.0.0",
        "stylelint-config-styled-components": "^0.1.1",
        "stylelint-junit-formatter": "^0.2.2",
        "stylelint-processor-styled-components": "^1.8.0",
        "terser-webpack-plugin": "^2.2.1",
        "webpack": "^4.41.2",
        "webpack-bundle-analyzer": "^3.6.0",
        "webpack-cli": "^3.3.10"
    }
}
